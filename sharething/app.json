{
  "expo": {
    "name": "share-thing",
    "slug": "sharething",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.av.sharething",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.141243147234-9q7jthjbp891h88udgid7fbnso2pjm81"
        }
    }
  },
  "android": {
    "package": "com.av.sharething",
    "googleServicesFile": "./google-services.json"
}
}
}